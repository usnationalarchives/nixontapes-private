  <cpfDescription>
    <identity>
      <entityType>person</entityType>

            if ($entityType eq "person")
              then
              
            <nameEntry localType="marcfield:100">
        <part localType="marcfield:100$a"/>
        <part localType="marcfield:100$d"/>
        <authorizedForm>lcnaf</authorizedForm>
        <!-- <authorizedForm>VIAF</authorizedForm>
                <authorizedForm>WorldCat</authorizedForm>
                <authorizedForm>USNARA-LCDRG</authorizedForm> -->
      </nameEntry>
      <nameEntry localType="nixonNames">
        <part localType="familyName"/>
        <part localType="givenName"/>
        <part localType="nickname"/>
        <part localType="maiden name"/>
        <part localtype="generational marker"/>
        <part localType="military rank or official title"/>
        <part localType="Mrs."/>
        <authorizedForm>NixonTapesIndex</authorizedForm>
      </nameEntry>
      <nameEntry localType="marcfield:400">
        <part localType="marcfield:400$a"/>
        <alternativeForm>USNARA-LCDRG</alternativeForm>
      </nameEntry>
      <nameEntry localType="marcfield:400">
        <part localType="marcfield:400$a"/>
        <alternativeForm>USNARA-LCDRG</alternativeForm>
      </nameEntry>
            

      else
      <nameEntry localType="marcfield:110">
        <part localType="marcfield:110$a"/>
        <part localType="marcfield:110$d"/>
        <authorizedForm>lcnaf</authorizedForm>
        <!-- <authorizedForm>VIAF</authorizedForm>
                <authorizedForm>WorldCat</authorizedForm>
                <authorizedForm>USNARA-LCDRG</authorizedForm> -->
      </nameEntry>
      <nameEntry localType="nixonNames">
        <part localType="directOrder"/>
        <alternativeForm>NixonTapesIndex</alternativeForm>
      </nameEntry>
      <nameEntry localType="marcfield:410">
        <part localType="marcfield:410$a"/>
        <alternativeForm>USNARA-LCDRG</alternativeForm>
      </nameEntry>
      <nameEntry localType="marcfield:410">
        <part localType="marcfield:410$a"/>
        <alternativeForm>USNARA-LCDRG</alternativeForm>
      </nameEntry>
    </identity>
